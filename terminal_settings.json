{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        // ペインに進む、戻る
        {
          "command": { "action": "moveFocus", "direction": "nextInOrder" },
          "keys": "alt+o"
        },
        {
          "command": { "action": "moveFocus", "direction": "previousInOrder" },
          "keys": "alt+shift+o"
        },
        // ペイン交換系
        {
            "command": { "action": "swapPane", "direction": "down" },
            "keys": "alt+down"
        },
        {
            "command": { "action": "swapPane", "direction": "left" },
            "keys": "alt+left"
        },
        {
            "command": { "action": "swapPane", "direction": "right" },
            "keys": "alt+right"
        },
        {
            "command": { "action": "swapPane", "direction": "up" },
            "keys": "alt+up"
        },
        // ペインの複製
        {
            "command": 
            { 
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate" 
            },
            "keys": "alt+shift+c"
        },
        // 文字サイズを増やす（ズームイン）
        {
            "command": { "action": "adjustFontSize", "delta": 1 },
            "keys": "shift+alt+plus"
        },
        // 文字サイズを減らす（ズームアウト）
        {
            "command": { "action": "adjustFontSize", "delta": -1 },
            "keys": "shift+alt+minus"
        },
        // フォントサイズをリセット
        {
            "command": { "action": "resetFontSize" },
            "keys": "ctrl+0"
        },
        // ペイン垂直分割
        {
            "command": { "action": "splitPane", "split": "vertical" },
            "keys": "alt+shift+v"
        },
        // ペイン水平分割
        {
            "command": { "action": "splitPane", "split": "horizontal" },
            "keys": "alt+shift+h"
        },
        // タブを左に移動
        { 
            "command": 
            { 
                "action": "moveTab",
                "direction": "backward"
            },
            "keys": "shift+alt+<"
        },
        // タブを右に移動
        { 
            "command": { 
                "action": "moveTab",
                "direction": "forward"
            },
            "keys": "shift+alt+>"
        },
        // フォーカスを下に移動
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+j"
        },
        // フォーカスを上に移動
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+k"
        },
        // フォーカスを右に移動
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+l"
        },
        // フォーカスを左に移動
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+h"
        },
        // pane リサイズ
        { "command": { "action": "resizePane", "direction": "down" }, "keys": "alt+ctrl+j" },
        { "command": { "action": "resizePane", "direction": "left" }, "keys": "alt+ctrl+h" },
        { "command": { "action": "resizePane", "direction": "right" }, "keys": "alt+ctrl+l" },
        { "command": { "action": "resizePane", "direction": "up" }, "keys": "alt+ctrl+k" },
        // 画面スクロール系
        {
            "command": 
            {
                "action": "scrollUp"
            },
            "keys": "shift+alt+k"
        },
        {
            "command": "scrollUpPage",
            "keys": "shift+alt+b"
        },
        {
            "command": 
            {
                "action": "scrollDown"
            },
            "keys": "shift+alt+j"
        },
        {
            "command": "scrollDownPage",
            "keys": "shift+alt+f"
        },
        // 自動画面分割
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+p"
        },
        // 画面delete
        { "command": "closePane", "keys": "alt+w" },
        {
            "command": 
            {
                "action": "closeOtherTabs"
            },
            "keys": "ctrl+alt+o"
        },
        {
            "command": "switchSelectionEndpoint",
            "keys": "space"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 0
            },
            "keys": "ctrl+numpad1"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 1
            },
            "keys": "ctrl+numpad2"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 2
            },
            "keys": "ctrl+numpad3"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 3
            },
            "keys": "ctrl+numpad4"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 4
            },
            "keys": "ctrl+numpad5"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 5
            },
            "keys": "ctrl+numpad6"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 6
            },
            "keys": "ctrl+numpad7"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 7
            },
            "keys": "ctrl+numpad8"
        },
        {
            "command": 
            {
                "action": "switchToTab",
                "index": 8
            },
            "keys": "ctrl+numpad9"
        }
    ],
